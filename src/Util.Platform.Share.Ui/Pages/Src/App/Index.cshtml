@page

<util-card>
<util-row >
    <util-input name="icon" bind-add-on-after="selectIcon" />
    <util-template id="selectIcon">
        <platform-icon-select columns="6" rows="5" pageSize="50" [pageSizeOptions]="[10,20,30,40,50]" p [selectedIcon]="model.icon" [visible]="selectIconVisible" (selectIcon)="selectedIcon($event)"></platform-icon-select>
    </util-template>
</util-row>

</util-card>